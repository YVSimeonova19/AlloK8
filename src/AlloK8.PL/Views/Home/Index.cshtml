@page
@model IActionResult

@{
    Layout = "_Layout";
}

<div class="content-wrapper">
    <div class="row">
        <div class="col-md-12 grid-margin">
            <div class="row">
                <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                    <h3 class="font-weight-bold">Welcome [Name]</h3>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        @await Html.PartialAsync("_StatusBlocks")
        
        @await Html.PartialAsync("_ToDoList")
    </div>
    <div class="row">
        @await Html.PartialAsync("_AdvancedTable")
    </div>
</div>